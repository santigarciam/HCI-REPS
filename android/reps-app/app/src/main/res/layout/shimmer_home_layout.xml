<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/loader10"
        android:layout_width="200dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/loader6"
        android:layout_width="200dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="55dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loader2" />

    <ImageView
        android:id="@+id/loader2"
        android:layout_width="277dp"
        android:layout_height="181dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintBottom_toBottomOf="@+id/loader4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loader10" />

    <ImageView
        android:id="@+id/loader14"
        android:layout_width="277dp"
        android:layout_height="181dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loader6" />

    <ImageView
        android:id="@+id/loader4"
        android:layout_width="107dp"
        android:layout_height="183dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/loader2"
        app:layout_constraintTop_toBottomOf="@+id/loader" />

    <ImageView
        android:id="@+id/loader15"
        android:layout_width="107dp"
        android:layout_height="183dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/loader14"
        app:layout_constraintTop_toBottomOf="@+id/loader13" />

    <ImageView
        android:id="@+id/loader"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:layout_marginEnd="16dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintBottom_toBottomOf="@+id/loader10"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/loader13"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:layout_marginEnd="16dp"
        android:elevation="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/fondo_gris"
        app:layout_constraintBottom_toBottomOf="@+id/loader6"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>